package com.example.ObjectOnTrack;

import com.example.GeneticAlgorithm.Weapon;

public class ObjectOnTrack {
	private int posX;
	private int posY;
	private Weapon weapon;
	
	public ObjectOnTrack (int pPosX, int pPosY) {
		posX = pPosX;
		posY = pPosY;
		weapon = new Weapon();
	}
	
	public int getX() {
		return posX;
	}
	
	public int getY() {
		return posY;
	}
	
	public void setWeapon (Weapon pWeapon) {
		weapon = pWeapon;
	}
}
